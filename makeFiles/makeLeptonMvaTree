CC=g++ -O3
CFLAGS= -Wl,--no-as-needed
LDFLAGS=`root-config --glibs --cflags`
SOURCES= src/eventSelection.cc src/objectSelection.cc src/kinematicTools.cc src/treeReaderErrors.cc leptonMvaTraining/leptonMvaTree.cc src/treeReader.cc src/analysisTools.cc src/TrainingTree.cc src/Sample.cc src/Category.cc
OBJECTS=$(SOURCES:.cc=.o)
EXECUTABLE= leptonMvaTree

all: 
	$(CC) $(CFLAGS) $(SOURCES) $(LDFLAGS) -o $(EXECUTABLE)
	
clean:
	rm -rf *o $(EXECUTABLE)b
