CC=g++
CFLAGS= -Wl,--no-as-needed
LDFLAGS=`root-config --glibs --cflags`
SOURCES= ../src/treeReader.cc ../src/ewkino.cc ../src/analysisTools.cc ../src/eventSelection.cc ../src/ewkinoTools.cc ../src/trilepTools.cc ../plotting/plotCode.cc ../plotting/tdrStyle.cc
OBJECTS=$(SOURCES:.cc=.o)
EXECUTABLE= ../ewkino

all: 
	$(CC) $(CFLAGS) $(SOURCES) $(LDFLAGS) -o $(EXECUTABLE)
	
clean:
	rm -rf *o $(EXECUTABLE)
